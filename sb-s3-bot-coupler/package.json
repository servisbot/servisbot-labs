{
  "name": "webspray",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preDeploy": "rm -rf node_modules && npm i --prod --quiet",
    "postDeploy": "npm i --quiet",
    "deploy": "npm run preDeploy && sls deploy --region us-east-1 --sb_region us-1 --organization YOUR_ORGANIZATION_HERE --api_key YOUR_API_KEY_HERE --bot_endpoint YOU_BOT_ENDPOINT_HERE && npm run postDeploy",
    "lint": "node_modules/.bin/eslint src test",
    "report": "nyc report --reporter=lcov",
    "test": "nyc ./node_modules/.bin/mocha --recursive test/*",
    "depcheck": "depcheck . --ignores='serverless*, pre-commit'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/servisbot/WebSpray.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/servisbot/WebSpray/issues"
  },
  "homepage": "https://github.com/servisbot/WebSpray#readme",
  "devDependencies": {
    "aws-sdk": "^2.552.0",
    "clone": "^2.1.2",
    "depcheck": "^0.8.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-mocha": "^5.1.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "ramda": "^0.27.0",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "include": "src",
    "check-coverage": true,
    "per-file": false,
    "lines": 94,
    "statements": 94,
    "functions": 95,
    "branches": 94,
    "all": true
  },
  "pre-commit": [
    "lint",
    "test",
    "depcheck"
  ],
  "dependencies": {
    "aws-serverless-express": "^3.3.8",
    "express": "^4.17.1",
    "loglevel": "^1.6.8",
    "mailparser": "^2.7.7",
    "node-fetch": "^2.6.0",
    "serverless-parameters": "^0.1.0",
    "serverless-pseudo-parameters": "^2.5.0"
  }
}
